%h1 Listing interpretations

%table
  %tr
    %th Text
    %th Card code
    %th
    %th
    %th

  - @interpretations.each do |interpretation|
    %tr
      %td= interpretation.text
      %td= interpretation.card_code
      %td= link_to 'Show', interpretation
      %td= link_to 'Edit', edit_interpretation_path(interpretation)
      %td= link_to 'Destroy', interpretation, method: :delete, data: { confirm: 'Are you sure?' }

%br

= link_to 'New Interpretation', new_interpretation_path
